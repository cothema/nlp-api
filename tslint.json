{
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier",
    "tslint-etc"
  ],
  "jsRules": {
  },
  "rules": {
    "max-line-length": [
      true,
      140
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "no-console": false,
    "no-empty": false,
    "no-unused-declaration": true,
    "object-literal-sort-keys": false,
    "prettier": true,
    "variable-name": {
      "options": [
        "allow-leading-underscore"
      ]
    },
    "no-conditional-assignment": false
  },
  "rulesDirectory": []
}
